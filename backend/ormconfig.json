[{
    "name": "default",
    "type": "sqlite",
    "entities": ["./src/models/*.ts"],
    "migrations": ["./src/database/migrations/*.ts"],
    "database" : "./src/database/database.db",
    "cli": {
      "migrationsDir": "./src/database/migrations/"
    },
    
     "synchronize": false,
     "options": {
        "encrypt": true,
        "enableArithAbort": true
      }
  }]